{
  "name": "leaps-trading-system",
  "version": "1.0.0",
  "description": "Production-ready LEAPS options trading system",
  "main": "dist/server/app.js",
  "scripts": {
    "dev": "tsx watch src/server/app.ts",
    "build": "tsc",
    "start": "node dist/server/app.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "review": "tsx src/agents/runCodeReview.ts",
    "review:quick": "tsx src/agents/runCodeReview.ts --quick",
    "docker:build": "docker build -t leaps-trading .",
    "docker:run": "docker-compose up",
    "migration:run": "npm run build && node dist/migrations/run.js"
  },
  "dependencies": {
    "@master-chief/alpaca": "^6.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "lru-cache": "^10.0.0",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.0",
    "prom-client": "^15.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.2.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
